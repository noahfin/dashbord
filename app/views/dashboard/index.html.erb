   </div>
   <div class="col-lg-3 col-md-6 col-sm-6 first-show-card">
      <%= link_to deals_path do %>
      <div class="card card-stats card-dash-rose rounded-0  card-1">
         <div class="card-header card-header-icon ">
            <div class="card-icon card-icon-outline-danger">
               <i class="material-icons bg-black text-danger">equalizer</i>
            </div>
            <p class="card-category card-top-dash text-white">Deals</p>
            <h3 class="card-title text-white"><%= Deal.count %></h3>
         </div>
         <div class="card-footer text-white">
            <div class="stats text-white">
               <i class="material-icons  text-danger mt-2 pt-1 pr-3 ">local_offer</i> View All Deals
            </div>
         </div>
      </div>
      <% end %>
   </div>
   <div class="col-lg-3 col-md-6 col-sm-6 first-show-card">
      <div class="card card-stats  card-dash-success rounded-0 card-1" >
         <div class="card-header  card-header-icon">
            <div class="card-icon  card-icon-outline-success">
               <i class="material-icons bg-black text-success">store</i>
            </div>
            <p class="card-category card-top-dash text-wgite">Revenue</p>
