<% flash.each do |name, msg| %>
  <% if msg.is_a?(String) %>
  <% name = "info" if !name %>
    <script>
      $(function() {
        $.notify({
      title: '<strong>Heads up!</strong>',
      message: '<%= msg.to_s %>'
    },{
      type: '<%= name.to_s %>',
      delay: .1

    });
    });
    </script>
  <% end %>
<% end %>






